{
  "name": "opinion-back-testing",
  "version": "1.0.0",
  "description": "Opinion 예측시장 아비트라지 기회 포착 시스템",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "pm2:build": "npm run build",
    "pm2:start": "npm run pm2:build && pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop polymarket-arbitrage",
    "pm2:restart": "npm run pm2:build && pm2 restart polymarket-arbitrage",
    "pm2:delete": "pm2 delete polymarket-arbitrage",
    "pm2:logs": "pm2 logs polymarket-arbitrage",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "opinion",
    "arbitrage",
    "prediction-market"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "node-cron": "^3.0.3",
    "typeorm": "^0.3.17",
    "mysql2": "^3.6.5",
    "reflect-metadata": "^0.1.13",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "pm2": "^5.3.0"
  }
}
